{
  "source": "./app/components",
  "destination": "./docs",
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "unexportIdentifier": {
          "enable": true
        }
      }
    },
    {
      "name": "esdoc-jsx-plugin",
      "option": {
        "enable": true
      }
    },
    {
      "name": "esdoc-ecmascript-proposal-plugin",
      "option": {
        "all": true
      }
    },
    {
      "name": "esdoc-typescript-plugin",
      "option": {
        "enable": true
      }
    }
  ]
}